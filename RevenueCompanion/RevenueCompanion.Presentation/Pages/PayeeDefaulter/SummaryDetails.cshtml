@page
@model RevenueCompanion.Presentation.Pages.SummaryReport.SummaryDetailsModel
@{
    var payerUtin = ViewBag.payerUtin;
    var taxAgent = ViewBag.taxAgent;
}
<input type="hidden" value="@payerUtin" id="payerUtin" />
<div class="row">
    <div class="col-md-12">
        <div class="panel panel-default">
            <div class="panel-heading">
                <div class="row">
                    <div class="col-md-11 col-sm-10 col-xl-8">
                        <h3>Summary Details</h3>
                        <p>@taxAgent (@payerUtin)</p>
                    </div>
                    <div class="col-md-1 col-sm-2 col-xl-4">
                        <a href="/PayeeDefaulter/SummaryReport" class="btn btn-primary"><i class="ion-arrow-return-left mr-2"></i> Back</a>
                    </div>
                </div>

            </div>
            <br />
            <div id="requestContainer"></div>

        </div>
    </div>
    <div id="cover-spin"></div>
</div>

@section Scripts{
    <script src="~/appscripts/summaryDetails.js" asp-append-version="true"></script>
}